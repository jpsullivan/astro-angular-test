<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient (sky) -->
  <defs>
    <linearGradient id="sky" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87ceeb;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e0f2fe;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="water" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:0.7" />
      <stop offset="100%" style="stop-color:#1e40af;stop-opacity:0.9" />
    </linearGradient>
  </defs>

  <rect width="800" height="600" fill="url(#sky)"/>

  <!-- Mountains -->
  <polygon points="0,300 200,150 400,250 600,100 800,280 800,600 0,600" fill="#64748b" opacity="0.6"/>
  <polygon points="100,350 300,200 500,320 700,180 800,350 800,600 0,600" fill="#475569" opacity="0.7"/>

  <!-- Hills -->
  <ellipse cx="200" cy="450" rx="250" ry="100" fill="#10b981" opacity="0.6"/>
  <ellipse cx="600" cy="470" rx="280" ry="90" fill="#059669" opacity="0.6"/>

  <!-- River path (winding) -->
  <path d="M 100 250 Q 200 280 250 350 T 400 420 T 550 480 T 700 540"
        stroke="url(#water)"
        stroke-width="60"
        fill="none"
        opacity="0.8"/>

  <!-- River highlights -->
  <path d="M 100 250 Q 200 280 250 350 T 400 420 T 550 480 T 700 540"
        stroke="#60a5fa"
        stroke-width="30"
        fill="none"
        opacity="0.4"/>

  <!-- Ocean at the end -->
  <ellipse cx="700" cy="560" rx="150" ry="40" fill="#1e40af" opacity="0.8"/>

  <!-- Trees -->
  <circle cx="150" cy="400" r="25" fill="#22c55e" opacity="0.7"/>
  <rect x="145" y="400" width="10" height="40" fill="#78350f"/>

  <circle cx="520" cy="420" r="30" fill="#22c55e" opacity="0.7"/>
  <rect x="514" y="420" width="12" height="45" fill="#78350f"/>

  <!-- Title overlay -->
  <rect x="200" y="30" width="400" height="60" fill="#1e293b" opacity="0.8" rx="8"/>
  <text x="400" y="60" font-family="Arial, sans-serif" font-size="20" font-weight="bold" text-anchor="middle" fill="white">
    The River's Predetermined Path
  </text>
  <text x="400" y="80" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="#cbd5e1">
    Yet it still reaches the ocean
  </text>
</svg>
